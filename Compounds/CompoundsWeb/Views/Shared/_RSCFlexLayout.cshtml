@*/* *********************************************************************
        DO NOT EDIT THIS FILE FROM ANY SOLUTION EXCEPT CHEMSPIDER.LAYOUT!!!
        OTHERWISE IT WILL BE OVERWRITTEN WHEN PACKAGE UPDATED
    ******************************************************************* */*@

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />

    @Styles.Render("~/Content/themes/base/css-jqueryui")
    @Styles.Render("~/Content/css/css-flex-website")
    @Styles.Render("~/Content/css-bootstrap")


    @RenderSection("css", required: false)

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body class="flex-layout">
    <div class="rsc-header">
        @Html.RSCHeader()
    </div>
    <div class="chemspider-header">
        <a href="~/" class="site-logo"><img src="~/Images/website-logo.png" /></a>
        <a href="http://www.rsc.org" class="rsc-logo"><img src="~/Images/rsc-logo.png" /></a>
    </div>
    <div class="chemspider-content">
        @RenderSection("featured", required: false)
        @RenderBody()
    </div>

    <div class="rsc-footer">
        @Html.RSCFooter()
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
